{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex-M4 Debug",
            "cwd": "${workspaceRoot}",
            "executable": "./src/armgcc/debug/freertos_hello.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink",
            "device": "MCIMX7U5_M4",
            "interface": "swd",
            "ipAddress": null,
            "serialNumber": null,
            "overrideLaunchCommands": [
                "-enable-pretty-printing",
                "set remotetimeout 15",
                "monitor reset ",
                "monitor halt",
                "load",
                "monitor go",
            ],
        }
    ]
}